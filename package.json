{
    "name": "ag-gun-app",
    "version": "0.0.2",
    "description": "Angular wrapper for Gun",
    "keywords": [
        "angular",
        "gun",
        "rxjs"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivkan/ag-gun.git"
    },
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "start:gun": "node server",
        "build_lib": "ng build --prod ag-gun",
        "copy-license": "cp ./LICENSE ./dist/ag-gun",
        "copy-readme": "cp ./README.md ./dist/ag-gun",
        "copy-files": "npm run copy-license && npm run copy-readme",
        "npm_pack": "cd dist/ag-gun && npm pack",
        "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
        "test": "ng test",
        "test_lib": "ng test ag-gun",
        "lint": "ng lint",
        "lint_lib": "ng lint ag-gun",
        "e2e_lib": "ng e2e",
        "e2e": "ng e2e ag-gun"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~7.0.0",
        "@angular/cdk": "^7.0.2",
        "@angular/common": "~7.0.0",
        "@angular/compiler": "~7.0.0",
        "@angular/core": "~7.0.0",
        "@angular/flex-layout": "^7.0.0-beta.19",
        "@angular/forms": "~7.0.0",
        "@angular/http": "~7.0.0",
        "@angular/material": "^7.0.2",
        "@angular/platform-browser": "~7.0.0",
        "@angular/platform-browser-dynamic": "~7.0.0",
        "@angular/router": "~7.0.0",
        "core-js": "^2.5.4",
        "gun": "^0.2019.416",
        "hammerjs": "^2.0.8",
        "lodash": "^4.17.19",
        "rxjs": "~6.3.3",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "serialize-javascript": ">=2.1.1",
        "set-value": ">=2.0.1",
        "tar": ">=2.2.2",
        "webpack-dev-server": ">=3.1.11",
        "@angular-devkit/build-angular": "~0.10.0",
        "@angular-devkit/build-ng-packagr": "~0.10.0",
        "@angular/cli": "~7.0.4",
        "@angular/compiler-cli": "~7.0.0",
        "@angular/language-service": "~7.0.0",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/lodash": "^4.14.117",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^4.2.0",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tsickle": ">=0.29.0",
        "tslib": "^1.9.0",
        "tslint": "~5.11.0",
        "typescript": "~3.1.1"
    }
}
